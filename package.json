{
	"name": "ocaml-formatter",
	"displayName": "OCaml Formatter",
	"description": "Formatter for OCaml language",
	"version": "0.2.0",
	"license": "Apache-2.0",
	"publisher": "badochov",
	"icon": "assets/logo.png",
	"categories": [
		"Formatters"
	],
	"keywords": [
		"ocaml",
		"format",
		"formatting",
		"formatter",
		"merlin"
	],
	"bugs": {
		"url": "https://github.com/badochov/ocamlformatter-vscode/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/badochov/ocamlformatter-vscode.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"activationEvents": [
		"onLanguage:ocaml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "OCaml Formatter",
			"properties": {
				"ocaml-formatter.profile": {
					"type": "string",
					"default": "conventional",
					"enum": [
						"conventional",
						"janestreet",
						"sparse",
						"compact",
						"ocamlformat"
					],
					"description": "Choose formatting profile"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
